@model IEnumerable<SalonDeBellezaCarlitos.WebUI.Models.DepartamentoViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.depa_Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.depa_Codigo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.depa_FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.depa_FechaModificacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.depa_Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.depa_Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.depa_FechaCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.depa_FechaModificacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.depa_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.depa_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.depa_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
