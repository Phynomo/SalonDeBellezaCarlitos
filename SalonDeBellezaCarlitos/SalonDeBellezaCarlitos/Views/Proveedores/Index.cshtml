@model IEnumerable<SalonDeBellezaCarlitos.WebUI.Models.ProveedorViewModel>

    @{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h1>Index</h1>

    <p>
        <a asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.prov_NombreEmpresa)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.prov_NombreContacto)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.prov_DireccionExacta)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.prov_Telefono)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.prov_FechaCreacion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.prov_UsuarioCreacion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.prov_FechaModificacion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.prov_UsuarioModificacion)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_NombreEmpresa)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_NombreContacto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_DireccionExacta)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_Telefono)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_FechaCreacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_UsuarioCreacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_FechaModificacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.prov_UsuarioModificacion)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.prov_Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.prov_Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.prov_Id">Delete</a>
                </td>
            </tr>
}
        </tbody>
    </table>
