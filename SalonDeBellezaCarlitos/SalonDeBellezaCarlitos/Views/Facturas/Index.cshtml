@model IEnumerable<SalonDeBellezaCarlitos.WebUI.Models.FacturasViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fact_Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fact_FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fact_Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.clie_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.empl_Id_Atendido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.empl_Id_Caja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.metp_Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.fact_Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fact_FechaCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fact_FechaModificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fact_Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.clie_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.empl_Id_Atendido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.empl_Id_Caja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.metp_Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.fact_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.fact_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.fact_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
